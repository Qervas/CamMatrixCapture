{
  "application": {
    "name": "SaperaCapturePro",
    "version": "1.0.0",
    "description": "Professional Camera Capture System for Industrial Applications"
  },
  "server": {
    "httpPort": 8080,
    "enableWebInterface": true,
    "enableApiCors": true,
    "maxRequestSizeMB": 16,
    "documentRoot": "web"
  },
  "cameras": {
    "initializationTimeoutMs": 30000,
    "captureTimeoutMs": 10000,
    "enableParallelCapture": true,
    "maxConcurrentCaptures": 4,
    "enableBandwidthManagement": true,
    "maxTotalBandwidthMbps": 1000,
    "bandwidthSafetyMarginPercent": 10
  },
  "storage": {
    "defaultOutputDirectory": "captures",
    "imageFormat": "TIFF",
    "imageQuality": 95,
    "createTimestampFolders": true,
    "autoCleanupDays": 30
  },
  "logging": {
    "level": "INFO",
    "logToFile": true,
    "logToConsole": true,
    "maxLogFileSizeMB": 100,
    "maxLogFileCount": 10
  },
  "security": {
    "requireAuthentication": false,
    "apiKey": "",
    "allowedOrigins": ["*"],
    "rateLimitRequestsPerMinute": 1000
  },
  "monitoring": {
    "enableHealthCheck": true,
    "enableMetrics": true,
    "metricsRetentionHours": 24
  },
